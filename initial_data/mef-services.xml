<config xmlns="http://tail-f.com/ns/config/1.0">
  <mef-services xmlns="urn:mef:yang:mef-legato-services">
    <carrier-ethernet>
      <subscriber-services>
        <evc>
          <evc-id>sd-wan</evc-id>
          <cos-names>
            <cos-name>
              <name>cosn0</name>
            </cos-name>
          </cos-names>
          <end-points>
            <end-point>
              <uni-id>uni-kvantel</uni-id>
              <color-identifier>colorp0</color-identifier>
              <cos-identifier>cos0</cos-identifier>
              <eec-identifier>eec0</eec-identifier>
              <ce-vlans>
                <ce-vlan>1</ce-vlan>
              </ce-vlans>
              <ingress-bwp-per-evc>
                <bw-profile-flow-parameters>BANDWIDTH_LTE</bw-profile-flow-parameters>
              </ingress-bwp-per-evc>
            </end-point>
            <end-point>
              <uni-id>uni-telia1</uni-id>
              <color-identifier>colorp0</color-identifier>
              <cos-identifier>cos0</cos-identifier>
              <eec-identifier>eec0</eec-identifier>
              <ce-vlans>
                <ce-vlan>1</ce-vlan>
              </ce-vlans>
              <ingress-bwp-per-evc>
                <bw-profile-flow-parameters>BANDWIDTH_LTE</bw-profile-flow-parameters>
              </ingress-bwp-per-evc>
            </end-point>
            <end-point>
              <uni-id>uni-telia2</uni-id>
              <color-identifier>colorp0</color-identifier>
              <cos-identifier>cos0</cos-identifier>
              <eec-identifier>eec0</eec-identifier>
              <ce-vlans>
                <ce-vlan>1</ce-vlan>
              </ce-vlans>
              <ingress-bwp-per-evc>
                <bw-profile-flow-parameters>BANDWIDTH_LTE</bw-profile-flow-parameters>
              </ingress-bwp-per-evc>
            </end-point>
          </end-points>
          <connection-type>multipoint-to-multipoint</connection-type>
          <max-frame-size>2000</max-frame-size>
          <max-num-of-evc-end-point>3</max-num-of-evc-end-point>
        </evc>
      </subscriber-services>
    </carrier-ethernet>
  </mef-services>
</config>
